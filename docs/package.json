    "autoprefixer": "^10.4.20",
    "bits-ui": "1.3.19",
    "clsx": "^2.1.1",
    "cssnano": "^7.0.6",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
